DROP TABLE IF EXISTS `templet_portal_sub_el`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `templet_portal_sub_el` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `borderCode` longtext,
  `cssCode` longtext,
  `mainCode` longtext,
  `topCode` longtext,
  `footerCode` longtext,
  `targetStr` varchar(255) DEFAULT NULL,
  `titleFormat` varchar(255) DEFAULT NULL,
  `majorLoopCodeInLump` longtext,
  `minorLoopCodeInLump` longtext,
  `searchAreaCode` longtext,
  `functionAreaCode` longtext,
  `userPanelCodeLogin` longtext,
  `userPanelCodeLogout` longtext,
  `exclusiveCode1` longtext,
  `exclusiveCode2` longtext,
  `exclusiveCode3` longtext,
  `exclusiveCode4` longtext,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=22 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

